{
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "dot-notation": 1,
    "valid-typeof": 2,
    "no-undef":"off"
  },
  "overrides": [
    {
      "files": ["*.jsx", "*.js"]
    }
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": true
  }
}
